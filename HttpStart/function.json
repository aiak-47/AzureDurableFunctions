{
  "scriptFile": "__init__.py",
  "bindings": [
    {
      "authLevel": "ANONYMOUS",
      "name": "req",
      "type": "httpTrigger",
      "direction": "in",
      "route": "orchestrators/{functionName}",
      "methods": [
        "post",
        "get"
      ]
    },
    {
      "name": "starter",
      "type": "orchestrationClient",
      "direction": "out",
      "orchestrationClient" : {
        "extendedSessionTimeoutInSeconds" : 86400
      }
    }
  ]
}